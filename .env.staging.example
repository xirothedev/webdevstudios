# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here_staging
POSTGRES_DB=webdevstudios_staging

# Redis
REDIS_PASSWORD=your_redis_password_here_staging

# JWT
JWT_SECRET=your_jwt_secret_here_staging
JWT_EXPIRES_IN=7d

# OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK_URL=https://staging.your-domain.com/api/auth/google/callback

GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_CALLBACK_URL=https://staging.your-domain.com/api/auth/github/callback

# AWS S3/R2 (use same or separate bucket for staging)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=auto
AWS_S3_BUCKET=your-bucket-name-staging
AWS_ENDPOINT=https://your-r2-endpoint.r2.cloudflarestorage.com

# Email (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
EMAIL_FROM=noreply@staging.your-domain.com

# Frontend
NEXT_PUBLIC_API_URL=https://staging.your-domain.com/api

# GitHub Container Registry
GITHUB_REPOSITORY=your-username/webdevstudios
